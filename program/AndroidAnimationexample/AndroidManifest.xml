<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.androidshivendra.androidanimationexample"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
         <activity
            android:name="in.androidshivendra.androidanimationexample.Test"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="in.androidshivendra.androidanimationexample.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="in.androidshivendra.androidanimationexample.BlinkActivity"></activity>
        <activity android:name="in.androidshivendra.androidanimationexample.BounceActivity"></activity>
        <activity android:name="in.androidshivendra.androidanimationexample.CrossfadeActivity"></activity>
        <activity android:name="in.androidshivendra.androidanimationexample.FadeInActivity"></activity>
        <activity android:name="in.androidshivendra.androidanimationexample.FadeOutActivity"></activity>
        <activity android:name="in.androidshivendra.androidanimationexample.MoveActivity"></activity>
        <activity android:name="in.androidshivendra.androidanimationexample.RotateActivity"></activity>
        <activity android:name="in.androidshivendra.androidanimationexample.SequentialActivity"></activity>
        <activity android:name="in.androidshivendra.androidanimationexample.SlideDownActivity"></activity>
        <activity android:name="in.androidshivendra.androidanimationexample.SlideUpActivity"></activity>
        <activity android:name="in.androidshivendra.androidanimationexample.TogetherActivity"></activity>
        <activity android:name="in.androidshivendra.androidanimationexample.ZoomInActivity"></activity>
        <activity android:name="in.androidshivendra.androidanimationexample.ZoomOutActivity"></activity>
        
    </application>

</manifest>
